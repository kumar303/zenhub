    "@testing-library/preact": "^3.2.4",
    "@types/node": "^20.10.5",
    "autoprefixer": "^10.4.16",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^3.2.4"
  }
